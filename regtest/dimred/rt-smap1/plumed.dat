d1: READ FILE=swiss.dat VALUES=c1
d2: READ FILE=swiss.dat VALUES=c2
d3: READ FILE=swiss.dat VALUES=c3

dd: COLLECT_FRAMES ARG=d1,d2,d3 CLEAR=50
weights: CUSTOM ARG=dd_logweights FUNC=exp(x) PERIODIC=NO
smap: SKETCHMAP ...
  NLOW_DIM=2 ARG=dd WEIGHTS=weights
  HIGH_DIM_FUNCTION={SMAP R_0=4 A=3 B=2}
  LOW_DIM_FUNCTION={SMAP R_0=4 A=1 B=2}
...

DUMPVECTOR ARG=smap.*,weights FILE=smap FMT=%8.4f STRIDE=50
