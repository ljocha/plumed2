ones: ONES SIZE=6
fmat: CONTACT_MATRIX GROUP=1-6 SWITCH={EXP D_0=0.2 R_0=0.1}
cc: MATRIX_VECTOR_PRODUCT ARG=fmat,ones
mat: CONTACT_MATRIX GROUP=1-6 SWITCH={CUBIC D_0=0.34 D_MAX=0.38}
dfs: DFSCLUSTERING ARG=mat 

dist1: CLUSTER_DISTRIBUTION CLUSTERS=dfs MAX={BETA=0.05} MORE_THAN={RATIONAL R_0=1}
dist: CLUSTER_DISTRIBUTION WEIGHTS=cc CLUSTERS=dfs MAX={BETA=0.05} MORE_THAN={RATIONAL R_0=1}

PRINT ARG=dist.*,dist1.* FILE=colvar FMT=%8.4f
